{
    "buildOptions": {
        "keyFile": "../../security/CamlBuilder.snk"
    },

    "configurations": {
        "Debug": {
            "buildOptions": {
                "define": [ "DEBUG", "TRACE" ]
            }
        },
        "Release": {
            "buildOptions": {
                "define": [ "RELEASE", "TRACE" ],
                "optimize": true
            }
        }
    },

    "commands": {
        "run-tests": "xunit.runner.dnx" 
    },

    "dependencies": {
        "CamlBuilder": "1.0.0-beta4",
        "xunit": "2.1.0",
        "xunit.runner.dnx": "1.0.0-rc2-build10015"
    },

    "frameworks": {
        "dnx451": {
            "frameworkAssemblies": {
                "System.Runtime":  "" 
            }
        }
    }
}