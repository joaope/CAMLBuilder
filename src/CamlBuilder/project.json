  "projectUrl": "https://github.com/joaope/camlbuilder",
  "summary": "Small and simple API which allows you to easily write CAML queries, in a declarative way.",
  "description": "CAMLBuilder allows developers to easily write CAML queries without long strings of XML. Strongly-typed and declarative code, queries can be logically constructed.",
  "tags": [ "caml", "declarative", "strongly-typed", "sharepoint", "query", "api" ],
  "requireLicenseAcceptance": false,
  "repository": {
    "url": "https://github.com/joaope/CamlBuilder.git"
  },

  "compilationOptions": {
    "keyFile": "../../security/CamlBuilder.snk"
  },

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },

  "frameworks": {
    "net451": {
    },
    "dotnet5.4": {
      "dependencies": {
        "System.Linq": "4.0.0",
        "System.Reflection": "4.0.10"
      }
    }
  }
}